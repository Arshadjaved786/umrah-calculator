{"ast":null,"code":"import{useState,useEffect}from\"react\";export default function useLocalStorage(key,initial){const[state,setState]=useState(()=>{try{const raw=localStorage.getItem(key);return raw?JSON.parse(raw):initial;}catch(e){return initial;}});useEffect(()=>{try{localStorage.setItem(key,JSON.stringify(state));}catch(e){}},[key,state]);return[state,setState];}","map":{"version":3,"names":["useState","useEffect","useLocalStorage","key","initial","state","setState","raw","localStorage","getItem","JSON","parse","e","setItem","stringify"],"sources":["C:/Users/Taj Tech/Downloads/umrah-calculator/src/hooks/useLocalStorage.js"],"sourcesContent":["import { useState, useEffect } from \"react\";\r\n\r\nexport default function useLocalStorage(key, initial) {\r\n  const [state, setState] = useState(() => {\r\n    try {\r\n      const raw = localStorage.getItem(key);\r\n      return raw ? JSON.parse(raw) : initial;\r\n    } catch (e) {\r\n      return initial;\r\n    }\r\n  });\r\n\r\n  useEffect(() => {\r\n    try {\r\n      localStorage.setItem(key, JSON.stringify(state));\r\n    } catch (e) {}\r\n  }, [key, state]);\r\n\r\n  return [state, setState];\r\n}\r\n"],"mappings":"AAAA,OAASA,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CAE3C,cAAe,SAAS,CAAAC,eAAeA,CAACC,GAAG,CAAEC,OAAO,CAAE,CACpD,KAAM,CAACC,KAAK,CAAEC,QAAQ,CAAC,CAAGN,QAAQ,CAAC,IAAM,CACvC,GAAI,CACF,KAAM,CAAAO,GAAG,CAAGC,YAAY,CAACC,OAAO,CAACN,GAAG,CAAC,CACrC,MAAO,CAAAI,GAAG,CAAGG,IAAI,CAACC,KAAK,CAACJ,GAAG,CAAC,CAAGH,OAAO,CACxC,CAAE,MAAOQ,CAAC,CAAE,CACV,MAAO,CAAAR,OAAO,CAChB,CACF,CAAC,CAAC,CAEFH,SAAS,CAAC,IAAM,CACd,GAAI,CACFO,YAAY,CAACK,OAAO,CAACV,GAAG,CAAEO,IAAI,CAACI,SAAS,CAACT,KAAK,CAAC,CAAC,CAClD,CAAE,MAAOO,CAAC,CAAE,CAAC,CACf,CAAC,CAAE,CAACT,GAAG,CAAEE,KAAK,CAAC,CAAC,CAEhB,MAAO,CAACA,KAAK,CAAEC,QAAQ,CAAC,CAC1B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}